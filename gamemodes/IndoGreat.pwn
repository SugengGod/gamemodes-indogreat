	if(!IsPlayerAdmin(playerid))
	{
	    return SCM(playerid, COLOR_WHITE, "Kamu tidak diizinkan untuk menggunakan command ini.");
	}

	for(new i = 0; i < MAX_HOUSES; i ++)
	{
