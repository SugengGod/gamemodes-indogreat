							RemovePlayerWeapon(playerid, weaponid);
							SM(playerid, COLOR_AQUA, "Kamu telah menyimpan [%s] di slot %i ke Backpackmu", GetWeaponNameEx(pData[playerid][bpWeapons][i]), i + 1);
