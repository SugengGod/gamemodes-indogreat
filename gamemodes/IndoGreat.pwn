				GameTextForPlayer(playerid, "~w~Penelepon Medis~n~~r~Pergi ke checkpoint.", 5000, 1);
                pData[playerid][pCP] = CHECKPOINT_MISC;
                new Float:ppos[3];
				GetPlayerPosEx(targetid, ppos[0], ppos[1], ppos[2]);
