	    if(cache_get_row_int(0, 1) && pData[playerid][pAdmin] < 7)
	    {
	        return SCM(playerid, COLOR_SYNTAX, "Pemain ini dibanned secara permanen. Permabans hanya dapat dicabut oleh Pemilik Komunitas.");
