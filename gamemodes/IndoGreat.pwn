				SAM(COLOR_YELLOW, "AdmWarning: %s telah mencoba masuk saat dibanned.", playername);
	        }
	        else
	        {
    			mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "SELECT uid FROM users WHERE username = '%s'", GetPlayerNameEx(extraid));
