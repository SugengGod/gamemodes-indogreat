		    if(!pData[i][pAdminDuty])
		    {
			    SetPlayerHealth(i, 100.0);
			    pData[i][pBrokenLeg] = 0;

			    if(GetArmor(i) < 100.0)
			    {
				    SetScriptArmour(i, 100.0);
				}
			}

		    SCM(i, COLOR_WHITE, "Admin telah menyembuhkan semua orang di dekatnya.");
		}
	}

	return 1;
}

