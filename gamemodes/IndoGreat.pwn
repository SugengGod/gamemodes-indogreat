	Log_Write("log_admin", "%s (uid: %i) didenda %s (uid: %i) senilai %s (%i persen), Alasan: %s", GetPlayerNameEx(playerid), pData[playerid][pID], GetPlayerNameEx(targetid), pData[targetid][pID], FormatNumber(amount), percent, reason);
