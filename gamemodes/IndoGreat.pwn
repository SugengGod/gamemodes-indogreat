		SCMf(playerid, COLOR_GREY2, "(Neon: %s) - (Bagasi: %i/3) - (Health: %.1f) - (Bensin: %i l)", neon, VehicleInfo[vehicleid][vTrunk], health, vehicleFuel[vehicleid]);
