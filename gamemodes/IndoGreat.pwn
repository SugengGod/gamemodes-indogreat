    if(!Iter_Contains(Trees, tid)) return 0;
    new label[100];
    
    if(TreeData[tid][treeLumber] > 0) 
	{
	    format(label, sizeof(label), "Tree (%d)\n{ffffff}Lumber: {3dcc3f}%d\n{ffffff}Use /lumber take.", tid, TreeData[tid][treeLumber]);
