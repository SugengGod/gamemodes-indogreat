	SCM(playerid, COLOR_GREEN, "Terluka - (/injuries):");
	foreach(new i : Player)
	{
		if(pData[i][pInjured])
		{
		    new accepted[24];
		    if(IsPlayerConnected(pData[i][pAcceptedEMS]))
