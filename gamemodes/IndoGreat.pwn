    else if(!strcmp(option, "locked", true))
	{
	    new locked;

	    if(sscanf(param, "i", locked) || !(0 <= locked <= 1))
	    {
	        return SCM(playerid, COLOR_LIGHTBLUE, "[USAGE]:{ffffff} '/editentrance' [entranceid] [locked] [0/1]");
